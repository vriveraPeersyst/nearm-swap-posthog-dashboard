# Production Environment Configuration Template
# Copy this file to .env.production and fill in your production values

NODE_ENV=production

# PostHog Configuration (REQUIRED)
POSTHOG_BASE_URL=https://app.posthog.com
POSTHOG_PROJECT_ID=your_production_project_id
POSTHOG_API_KEY=your_production_api_key

# Swap Event Configuration
SWAP_EVENT_NAME=swap
NETWORK_FILTER=mainnet

# Volume Calculation Settings
VOLUME_SIDE=in
VOLUME_PROP_IN=amount_in
VOLUME_PROP_OUT=amount_out

# Account Filtering (optional)
EXCLUDE_ACCOUNT_ID_PATTERNS=
EXCLUDE_ACCOUNT_IDS=

# Internal Prices API (REQUIRED for production)
PRICES_API_URL=https://your-production-prices-api.com/api/prices

# Query Configuration - Optimized for production
BATCH_SIZE=1000
MAX_EVENTS=0

# CoinGecko API (optional but recommended for fallback)
COINGECKO_API_KEY=your_production_coingecko_api_key

# Logging and Debug
DEBUG=false
